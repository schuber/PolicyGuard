# API配置
PORT=5000
NODE_ENV=development

# OpenAI API配置
OPENAI_API_KEY=your_openai_api_key_here
AI_MODEL=gpt-4
AI_MODEL_TYPE=openai  # 可选值: openai, claude, mistral

# Claude API配置（如果使用Claude）
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Mistral API配置（如果使用Mistral）
MISTRAL_API_KEY=your_mistral_api_key_here

# 缓存配置
USE_CACHE=true  # 是否使用缓存AI响应

# 日志配置
LOG_LEVEL=info  # 可选值: error, warn, info, debug

# 前端构建路径（生产环境）
FRONTEND_BUILD_PATH=../frontend/dist 